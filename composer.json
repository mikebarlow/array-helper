{
    "name": "snscripts/array-helper",
    "description": "Framework agnostic Array Helper",
    "keywords": ["Array", "Helper", "Framework Agnostic"],
    "homepage": "https://github.com/mikebarlow/array-helper",
    "license": "MIT",
    "authors": [
        {
            "name": "Mike Barlow",
            "email": "mike@mikebarlow.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "squizlabs/php_codesniffer": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Snscripts\\ArrayHelper\\": "src"
        },
        "classmap": ["src/ArrayUtil.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Snscripts\\ArrayHelper\\Tests\\": "tests"
        },
        "classmap": ["src/ArrayUtil.php"]
    }
}
